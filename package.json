{
  "name": "pushit",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts --dry-run",
    "build": "bun build src/index.ts --compile --outfile pushit --minify --bytecode",
    "setup": "bun run build && mkdir -p ~/.local/bin && mv pushit ~/.local/bin/ && mkdir -p ~/.config/pushit && cp .env ~/.config/pushit/.env",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"./*.json\" \"./*.md\""
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "^1.3.9",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
